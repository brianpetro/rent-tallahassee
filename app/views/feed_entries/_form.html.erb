<%= simple_form_for(@feed_entry) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :summary %>
    <%= f.input :url %>
    <%= f.input :published_at %>
    <%= f.input :guid %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
