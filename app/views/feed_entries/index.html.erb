<h1>Listing feed_entries</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Summary</th>
    <th>Url</th>
    <th>Published at</th>
    <th>Guid</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feed_entries.each do |feed_entry| %>
  <tr>
    <td><%= feed_entry.name %></td>
    <td><%= feed_entry.summary %></td>
    <td><%= feed_entry.url %></td>
    <td><%= feed_entry.published_at %></td>
    <td><%= feed_entry.guid %></td>
    <td><%= link_to 'Show', feed_entry %></td>
    <td><%= link_to 'Edit', edit_feed_entry_path(feed_entry) %></td>
    <td><%= link_to 'Destroy', feed_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feed entry', new_feed_entry_path %>

<div id="recent_episodes">
  <h3>Recent ASCIIcasts Episodes</h3>
  <ul>
  <% for entry in FeedEntry.all(:limit => 10, :order => "published_at desc") %>
    <li><%= link_to h(entry.name), entry.url %></li>
  <% end %>
  </ul>
</div>
