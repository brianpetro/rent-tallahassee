<h1>Listing classifieds</h1>

<table>
  <tr>
    <th>Headline</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @classifieds.each do |classified| %>
  <tr>
    <td><%= classified.headline %></td>
    <td><%= classified.content %></td>
    <td><%= link_to 'Show', classified %></td>
    <td><%= link_to 'Edit', edit_classified_path(classified) %></td>
    <td><%= link_to 'Destroy', classified, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Classified', new_classified_path %>
